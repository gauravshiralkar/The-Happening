	<h2> Rating Vs Review </h2>
		<canvas id="canvas1" height="500" width="1100"></canvas>

	<script>
		var barChartData = {
			labels :[<%=rating%>],
			datasets : [
				{
					fillColor : "red",
					strokeColor : "red",
					data : [<%=review_count%>]
				},
				{
					fillColor : "turquoise",
					strokeColor : "turquoise",
					data : [<%=rating%>]
				}
			]
			
		}
	
	var myLine = new Chart(document.getElementById("canvas1").getContext("2d")).Bar(barChartData);
	</script>
